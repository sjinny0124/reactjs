{
  "name": "hello",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "markdown-it": "^8.4.2",
    "moment": "^2.24.0",
    "react": "~16.3.0",
    "react-dom": "~16.3.0",
    "react-scripts": "2.1.3"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "src/**/*.{js,jsx,tx,tsx,json,css,scss,md}": [
        "prettier",
        "git add"
      ]
    },
    "ignore": [
      "node_modules/**",
      ".git/**"
    ]
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.16.1",
    "source-map-explorer": "^1.6.0"
  }
}
